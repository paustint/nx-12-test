{
  "version": 2,
  "projects": {
    "app-2": {
      "root": "apps/app-2",
      "sourceRoot": "apps/app-2/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/app-2",
            "index": "apps/app-2/src/index.html",
            "main": "apps/app-2/src/main.tsx",
            "polyfills": "apps/app-2/src/polyfills.ts",
            "tsConfig": "apps/app-2/tsconfig.app.json",
            "assets": [
              "apps/app-2/src/favicon.ico",
              "apps/app-2/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/app-2/src/environments/environment.ts",
                  "with": "apps/app-2/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "app-2:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "app-2:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/app-2/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/app-2"
          ],
          "options": {
            "jestConfig": "apps/app-2/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "app-2-e2e": {
      "root": "apps/app-2-e2e",
      "sourceRoot": "apps/app-2-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/app-2-e2e/cypress.json",
            "devServerTarget": "app-2:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "app-2:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/app-2-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "my-app": {
      "root": "apps/my-app",
      "sourceRoot": "apps/my-app/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
